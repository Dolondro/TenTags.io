<div class = 'contents'>

  <% if session.userID == thisfilter.createdBy then %>
    <div>
      <a href="<%= url_for("updatefilter",{filterlabel = thisfilter.title}) %>">Filter Settings</a>
    </div>
  <% end %>
  <div id='posts'>
    <% for i, post in pairs(posts) do %>
      <% render("views.st.postelement",{post = post}) %>
    <% end %>
  </div>
</div>
