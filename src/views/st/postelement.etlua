<div>
  <div class='post' >
    <a class ='postLink' href="<%= post.link -%>"> <%= post.title -%></a>
    </br>
      <a class='commentLink' href="<%= url_for('viewpost',{postID = post.id}) %>"> <%= post.commentCount -%> comments</a>
      <% for _,tag in pairs(post.tags) do %>
        <% if not tag.name:find('^meta') then %>
          <div class = 'tag'>
            <%= tag.name%>
          </div>
        <% end %>
      <% end %>
  </div>
</div>
